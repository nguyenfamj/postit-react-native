{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"top-0": {
		"style": {
			"top": 0
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-32": {
		"style": {
			"height": 128
		}
	},
	"h-40": {
		"style": {
			"height": 160
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	}
}